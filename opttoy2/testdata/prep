exec
CREATE TABLE a (x INT, y INT)

exec
CREATE TABLE b (x INT, z INT)

prep
SELECT a.y FROM a
----
scan (a (x, y)) [in=0-1 out=1]
  project:
    variable (a.y) [in=1 out=1]

prep
SELECT a.y+1 FROM a
----
scan (a (x, y)) [in=0-1 out=2]
  project:
    binary (+) [in=1 out=2]
      inputs:
        variable (a.y) [in=1 out=1]
        const (1)

prep
SELECT * FROM b WHERE b.z > 10
----
scan (b (x, z)) [in=0-1 out=0-1]
  project:
    variable (*) [in=0-1 out=0-1]
  filter:
    comp (>) [in=1]
      inputs:
        variable (b.z) [in=1 out=1]
        const (10)

prep
SELECT a.y, b.z FROM a JOIN b ON (a.x = b.x) WHERE a.x > 7 AND b.z = 3
----
inner join [in=0-3 out=1,3]
  project:
    variable (a.y) [in=1 out=1]
    variable (b.z) [in=3 out=3]
  filter:
    comp (=) [in=0,2]
      inputs:
        variable (a.x) [in=0 out=0]
        variable (b.x) [in=2 out=2]
    comp (>) [in=0]
      inputs:
        variable (a.x) [in=0 out=0]
        const (7)
    comp (=) [in=3]
      inputs:
        variable (b.z) [in=3 out=3]
        const (3)
  inputs:
    scan (a (x, y)) [in=0-1 out=0-1]
    scan (b (x, z)) [in=2-3 out=2-3]

prep
SELECT b.x FROM a, b WHERE a.x > 7 AND b.z = 3
----
inner join [in=0,2-3 out=2]
  project:
    variable (b.x) [in=2 out=2]
  filter:
    comp (>) [in=0]
      inputs:
        variable (a.x) [in=0 out=0]
        const (7)
    comp (=) [in=3]
      inputs:
        variable (b.z) [in=3 out=3]
        const (3)
    comp (=) [in=0,2]
      inputs:
        variable (a.x) [in=0 out=0]
        variable (b.x) [in=2 out=2]
  inputs:
    scan (a (x, y)) [in=0-1 out=0]
    scan (b (x, z)) [in=2-3 out=2-3]

prep
SELECT b.x FROM a NATURAL JOIN b WHERE a.x > 7 AND b.z = 3
----
inner join [in=0,2-3 out=2]
  project:
    variable (b.x) [in=2 out=2]
  filter:
    comp (>) [in=0]
      inputs:
        variable (a.x) [in=0 out=0]
        const (7)
    comp (=) [in=3]
      inputs:
        variable (b.z) [in=3 out=3]
        const (3)
    comp (=) [in=0,2]
      inputs:
        variable (a.x) [in=0 out=0]
        variable (b.x) [in=2 out=2]
  inputs:
    scan (a (x, y)) [in=0-1 out=0]
    scan (b (x, z)) [in=2-3 out=2-3]

prep
SELECT b.x FROM a JOIN b USING (x) WHERE a.x > 7 AND b.z = 3
----
inner join [in=0,2-3 out=2]
  project:
    variable (b.x) [in=2 out=2]
  filter:
    comp (>) [in=0]
      inputs:
        variable (a.x) [in=0 out=0]
        const (7)
    comp (=) [in=3]
      inputs:
        variable (b.z) [in=3 out=3]
        const (3)
    comp (=) [in=0,2]
      inputs:
        variable (a.x) [in=0 out=0]
        variable (b.x) [in=2 out=2]
  inputs:
    scan (a (x, y)) [in=0-1 out=0]
    scan (b (x, z)) [in=2-3 out=2-3]
