exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

memo
SELECT y FROM a
----
2: [project [0 1 -]]
1: [variable a.y]
0: [scan a [-]]

memo
SELECT a.y, b.z FROM a NATURAL JOIN b
----
9: [project [5 8 -]]
8: [list [6 7]]
7: [variable b.z]
6: [variable a.y]
5: [inner join [0 1 4]]
4: [comp (=) [2 3]]
3: [variable b.x]
2: [variable a.x]
1: [scan b [-]]
0: [scan a [-]]
