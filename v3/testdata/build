exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

build
SELECT y FROM a
----
project
  columns: a.y:1
  projections:
    variable (a.y) [in=1]
  inputs:
    scan
      columns: a.x:0 a.y:1

build
SELECT a.* FROM a
----
scan
  columns: a.x:0 a.y:1

build
SELECT * FROM (SELECT a.x FROM a)
----
project
  columns: a.x:0
  projections:
    variable (a.x) [in=0]
  inputs:
    scan
      columns: a.x:0 a.y:1

build
SELECT * FROM (SELECT a.x+1 FROM a)
----
project
  columns: .column1:2
  projections:
    binary (+) [in=0]
      inputs:
        variable (a.x) [in=0]
        const (1)
  inputs:
    scan
      columns: a.x:0 a.y:1

build
SELECT a.y, a.x FROM a
----
project
  columns: a.y:1 a.x:0
  projections:
    variable (a.y) [in=1]
    variable (a.x) [in=0]
  inputs:
    scan
      columns: a.x:0 a.y:1

build
SELECT a.y+1 FROM a
----
project
  columns: .column1:2
  projections:
    binary (+) [in=1]
      inputs:
        variable (a.y) [in=1]
        const (1)
  inputs:
    scan
      columns: a.x:0 a.y:1

build
SELECT * FROM b WHERE b.z > 10
----
scan
  columns: b.x:0 b.z:1*
  filters:
    comp (>) [in=1]
      inputs:
        variable (b.z) [in=1]
        const (10)

build
SELECT a.y, b.z FROM a JOIN b ON (a.x = b.x) WHERE a.x > 7 AND b.z = 3
----
project
  columns: a.y:1 b.z:3
  projections:
    variable (a.y) [in=1]
    variable (b.z) [in=3]
  inputs:
    inner join
      columns: a.x:0* a.y:1 b.x:2* b.z:3*
      filters:
        comp (=) [in=0,2]
          inputs:
            variable (a.x) [in=0]
            variable (b.x) [in=2]
        comp (>) [in=0]
          inputs:
            variable (a.x) [in=0]
            const (7)
        comp (=) [in=3]
          inputs:
            variable (b.z) [in=3]
            const (3)
      inputs:
        scan
          columns: a.x:0 a.y:1
        scan
          columns: b.x:2 b.z:3

build
SELECT b.x FROM a, b WHERE a.x > 7 AND b.z = 3
----
project
  columns: b.x:2
  projections:
    variable (b.x) [in=2]
  inputs:
    inner join
      columns: a.x:0* a.y:1 b.x:2 b.z:3*
      filters:
        comp (>) [in=0]
          inputs:
            variable (a.x) [in=0]
            const (7)
        comp (=) [in=3]
          inputs:
            variable (b.z) [in=3]
            const (3)
      inputs:
        scan
          columns: a.x:0 a.y:1
        scan
          columns: b.x:2 b.z:3

build
SELECT b.x FROM a NATURAL JOIN b WHERE a.x > 7 AND b.z = 3
----
project
  columns: {a,b}.x:0
  projections:
    variable (b.x) [in=0]
  inputs:
    inner join
      columns: {a,b}.x:0* a.y:1 b.z:3*
      filters:
        comp (=) [in=0,2]
          inputs:
            variable (a.x) [in=0]
            variable (b.x) [in=2]
        comp (>) [in=0]
          inputs:
            variable (a.x) [in=0]
            const (7)
        comp (=) [in=3]
          inputs:
            variable (b.z) [in=3]
            const (3)
      inputs:
        scan
          columns: a.x:0 a.y:1
        scan
          columns: b.x:2 b.z:3

build
SELECT b.x FROM a JOIN b USING (x) WHERE a.x > 7 AND b.z = 3
----
project
  columns: {a,b}.x:0
  projections:
    variable (b.x) [in=0]
  inputs:
    inner join
      columns: {a,b}.x:0* a.y:1 b.z:3*
      filters:
        comp (=) [in=0,2]
          inputs:
            variable (a.x) [in=0]
            variable (b.x) [in=2]
        comp (>) [in=0]
          inputs:
            variable (a.x) [in=0]
            const (7)
        comp (=) [in=3]
          inputs:
            variable (b.z) [in=3]
            const (3)
      inputs:
        scan
          columns: a.x:0 a.y:1
        scan
          columns: b.x:2 b.z:3

build
SELECT * FROM a LEFT JOIN b USING (x)
----
left join
  columns: {a,b}.x:0* a.y:1 b.z:3
  filters:
    comp (=) [in=0,2]
      inputs:
        variable (a.x) [in=0]
        variable (b.x) [in=2]
  inputs:
    scan
      columns: a.x:0 a.y:1
    scan
      columns: b.x:2 b.z:3

build
SELECT * FROM a RIGHT JOIN b USING (x)
----
right join
  columns: {a,b}.x:0* a.y:1 b.z:3
  filters:
    comp (=) [in=0,2]
      inputs:
        variable (a.x) [in=0]
        variable (b.x) [in=2]
  inputs:
    scan
      columns: a.x:0 a.y:1
    scan
      columns: b.x:2 b.z:3

build
SELECT * FROM a FULL JOIN b USING (x)
----
full join
  columns: {a,b}.x:0* a.y:1 b.z:3
  filters:
    comp (=) [in=0,2]
      inputs:
        variable (a.x) [in=0]
        variable (b.x) [in=2]
  inputs:
    scan
      columns: a.x:0 a.y:1
    scan
      columns: b.x:2 b.z:3

build
SELECT a.x, a.y FROM a UNION SELECT a.y, a.x FROM a
----
union
  columns: a.x:0 a.y:1
  inputs:
    scan
      columns: a.x:0 a.y:1
    project
      columns: a.y:1 a.x:0
      projections:
        variable (a.y) [in=1]
        variable (a.x) [in=0]
      inputs:
        scan
          columns: a.x:0 a.y:1
