exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

prep
SELECT * FROM (SELECT * FROM a) AS b
----
rename
  columns: b.x:0 b.y:1
  inputs:
    scan
      columns: a.x:0 a.y:1

prep
SELECT * FROM (SELECT * FROM a) AS b (e, f)
----
rename
  columns: b.e:0 b.f:1
  inputs:
    scan
      columns: a.x:0 a.y:1

prep
SELECT d.*, c.* FROM a AS c NATURAL JOIN b AS d
----
project
  columns: {c,d}.x:0 d.z:3 {c,d}.x:0 c.y:1
  projections:
    variable (d.x) [in=0]
    variable (d.z) [in=3]
    variable (c.x) [in=0]
    variable (c.y) [in=1]
  inputs:
    inner join
      columns: {c,d}.x:0* c.y:1 d.z:3
      filters:
        comp (=) [in=0,2]
          inputs:
            variable (c.x) [in=0]
            variable (d.x) [in=2]
      inputs:
        rename
          columns: c.x:0 c.y:1
          inputs:
            scan
              columns: a.x:0 a.y:1
        rename
          columns: d.x:2 d.z:3
          inputs:
            scan
              columns: b.x:2 b.z:3

prep
SELECT x AS e FROM a
----
project
  columns: a.e:0
  projections:
    variable (a.x) [in=0]
  inputs:
    scan
      columns: a.x:0 a.y:1

prep
SELECT * FROM (SELECT x AS e FROM a)
----
project
  columns: a.e:0
  projections:
    variable (a.x) [in=0]
  inputs:
    scan
      columns: a.x:0 a.y:1
