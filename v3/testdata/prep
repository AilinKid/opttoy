exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

prep
SELECT y FROM a
----
project [in=1 out=1]
  columns: a.y:1
  projections:
    variable (a.y) [in=1]
  inputs:
    scan [in=0-1 out=0-1]
      columns: a.x:0 a.y:1

prep
SELECT a.* FROM a
----
project [in=0-1 out=0-1]
  columns: a.x:0 a.y:1
  projections:
    variable (a.x) [in=0]
    variable (a.y) [in=1]
  inputs:
    scan [in=0-1 out=0-1]
      columns: a.x:0 a.y:1

prep
SELECT * FROM (SELECT a.x FROM a)
----
project [in=0 out=0]
  columns: a.x:0
  projections:
    variable (a.x) [in=0]
  inputs:
    project [in=0 out=0]
      columns: a.x:0
      projections:
        variable (a.x) [in=0]
      inputs:
        scan [in=0-1 out=0-1]
          columns: a.x:0 a.y:1

prep
SELECT * FROM (SELECT a.x+1 FROM a)
----
project [in=2 out=2]
  columns: .column1:2
  projections:
    variable (column1) [in=2]
  inputs:
    project [in=0 out=2]
      columns: .column1:2
      projections:
        binary (+) [in=0]
          inputs:
            variable (a.x) [in=0]
            const (1)
      inputs:
        scan [in=0-1 out=0-1]
          columns: a.x:0 a.y:1

prep
SELECT a.y, a.x FROM a
----
project [in=0-1 out=0-1]
  columns: a.y:1 a.x:0
  projections:
    variable (a.y) [in=1]
    variable (a.x) [in=0]
  inputs:
    scan [in=0-1 out=0-1]
      columns: a.x:0 a.y:1

prep
SELECT a.y+1 FROM a
----
project [in=1 out=2]
  columns: .column1:2
  projections:
    binary (+) [in=1]
      inputs:
        variable (a.y) [in=1]
        const (1)
  inputs:
    scan [in=0-1 out=0-1]
      columns: a.x:0 a.y:1

prep
SELECT * FROM b WHERE b.z > 10
----
project [in=0-1 out=0-1]
  columns: b.x:0 b.z:1
  projections:
    variable (b.x) [in=0]
    variable (b.z) [in=1]
  inputs:
    scan [in=0-1 out=0-1]
      columns: b.x:0 b.z:1*
      filters:
        comp (>) [in=1]
          inputs:
            variable (b.z) [in=1]
            const (10)

prep
SELECT a.y, b.z FROM a JOIN b ON (a.x = b.x) WHERE a.x > 7 AND b.z = 3
----
project [in=1,3 out=1,3]
  columns: a.y:1 b.z:3
  projections:
    variable (a.y) [in=1]
    variable (b.z) [in=3]
  inputs:
    inner join [in=0-3 out=0-3]
      columns: a.x:0* a.y:1 b.x:2* b.z:3*
      filters:
        comp (=) [in=0,2]
          inputs:
            variable (a.x) [in=0]
            variable (b.x) [in=2]
        comp (>) [in=0]
          inputs:
            variable (a.x) [in=0]
            const (7)
        comp (=) [in=3]
          inputs:
            variable (b.z) [in=3]
            const (3)
      inputs:
        scan [in=0-1 out=0-1]
          columns: a.x:0 a.y:1
        scan [in=2-3 out=2-3]
          columns: b.x:2 b.z:3

prep
SELECT b.x FROM a, b WHERE a.x > 7 AND b.z = 3
----
project [in=2 out=2]
  columns: b.x:2
  projections:
    variable (b.x) [in=2]
  inputs:
    inner join [in=0-3 out=0-3]
      columns: a.x:0* a.y:1 b.x:2 b.z:3*
      filters:
        comp (>) [in=0]
          inputs:
            variable (a.x) [in=0]
            const (7)
        comp (=) [in=3]
          inputs:
            variable (b.z) [in=3]
            const (3)
      inputs:
        scan [in=0-1 out=0-1]
          columns: a.x:0 a.y:1
        scan [in=2-3 out=2-3]
          columns: b.x:2 b.z:3

prep
SELECT b.x FROM a NATURAL JOIN b WHERE a.x > 7 AND b.z = 3
----
project [in=0 out=0]
  columns: {a,b}.x:0
  projections:
    variable (b.x) [in=0]
  inputs:
    inner join [in=0-3 out=0-1,3]
      columns: {a,b}.x:0* a.y:1 b.z:3*
      filters:
        comp (=) [in=0,2]
          inputs:
            variable (a.x) [in=0]
            variable (b.x) [in=2]
        comp (>) [in=0]
          inputs:
            variable (a.x) [in=0]
            const (7)
        comp (=) [in=3]
          inputs:
            variable (b.z) [in=3]
            const (3)
      inputs:
        scan [in=0-1 out=0-1]
          columns: a.x:0 a.y:1
        scan [in=2-3 out=2-3]
          columns: b.x:2 b.z:3

prep
SELECT b.x FROM a JOIN b USING (x) WHERE a.x > 7 AND b.z = 3
----
project [in=0 out=0]
  columns: {a,b}.x:0
  projections:
    variable (b.x) [in=0]
  inputs:
    inner join [in=0-3 out=0-1,3]
      columns: {a,b}.x:0* a.y:1 b.z:3*
      filters:
        comp (=) [in=0,2]
          inputs:
            variable (a.x) [in=0]
            variable (b.x) [in=2]
        comp (>) [in=0]
          inputs:
            variable (a.x) [in=0]
            const (7)
        comp (=) [in=3]
          inputs:
            variable (b.z) [in=3]
            const (3)
      inputs:
        scan [in=0-1 out=0-1]
          columns: a.x:0 a.y:1
        scan [in=2-3 out=2-3]
          columns: b.x:2 b.z:3

prep
SELECT a.x, a.y FROM a UNION SELECT a.y, a.x FROM a
----
union [in=0-1 out=0-1]
  columns: a.x:0 a.y:1
  inputs:
    project [in=0-1 out=0-1]
      columns: a.x:0 a.y:1
      projections:
        variable (a.x) [in=0]
        variable (a.y) [in=1]
      inputs:
        scan [in=0-1 out=0-1]
          columns: a.x:0 a.y:1
    project [in=0-1 out=0-1]
      columns: a.y:1 a.x:0
      projections:
        variable (a.y) [in=1]
        variable (a.x) [in=0]
      inputs:
        scan [in=0-1 out=0-1]
          columns: a.x:0 a.y:1
