exec
CREATE TABLE a (x INT PRIMARY KEY, y INT)
----
table a
  x NOT NULL
  y NULL
  (x) KEY

exec
CREATE TABLE b (x INT, y INT, z INT, PRIMARY KEY (z, x))
----
table b
  x NOT NULL
  y NULL
  z NOT NULL
  (z,x) KEY

exec
CREATE TABLE c (x INT UNIQUE)
----
table c
  x NULL
  (x) WEAK KEY

exec
CREATE TABLE d (x INT, UNIQUE (x))
----
table d
  x NULL
  (x) WEAK KEY

exec
CREATE TABLE e (x INT NOT NULL, UNIQUE (x))
----
table e
  x NOT NULL
  (x) KEY

exec
CREATE TABLE f (x INT REFERENCES a (y))
----
table f
  x NULL
  (x) -> a(y)

exec
CREATE TABLE g (x INT, y INT, FOREIGN KEY (x, y) REFERENCES b (z, y))
----
table g
  x NULL
  y NULL
  (x,y) -> b(z,y)

exec
CREATE TABLE h (x INT REFERENCES a)
----
table h
  x NULL
  (x) -> a(x)

exec
CREATE TABLE i (x INT, FOREIGN KEY (x) REFERENCES a)
----
table i
  x NULL
  (x) -> a(x)

prep
SELECT * FROM b
----
project [in=0-2 out=0-2]
  columns: b.x:0 b.y:1 b.z:2
  projections:
    variable (b.x) [in=0 out=0]
    variable (b.y) [in=1 out=1]
    variable (b.z) [in=2 out=2]
  inputs:
    scan [in=0-2 out=0-2]
      columns: b.x:0* b.y:1 b.z:2*
      key: 0,2
